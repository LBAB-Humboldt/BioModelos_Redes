.botonesperfil {
	position: absolute;
	padding-left: 50%;
	padding-left: -webkit-calc(50% - 107px);
	padding-left: -moz-calc(50% - 107px);
	padding-left: calc(50% - 107px);
}

.editperfil {
	width: 30px;
	height: 30px;
	display: block;
	background-image: url(<%= asset_path 'editperfil.png' %>);
	float: left;
	margin-right: 15px;
}

.editform {
	position: fixed;
	z-index: 200;
	margin: 0 auto;
	margin-left:-webkit-calc(50% - 151px);
	margin-left: -moz-calc(50% - 151px);
	margin-left: calc(50% - 151px);
	-webkit-box-shadow: 5px 5px 10px 0px rgba(130,130,130,1);
	-moz-box-shadow: 5px 5px 10px 0px rgba(130,130,130,1);
	box-shadow: 5px 5px 10px 0px rgba(130,130,130,1);

}

.funcperfil {
	width: 62px;
	height: 30px;
	display: none;
	background-image: url(<%= asset_path 'funcperfil.png' %>);
	float: left;
}

.funcperfil a {
	display: block;
	width: 31px;
	height: 30px;
	float: left;
}

.perfilline {
	width: 100%;
	background: url(<%= asset_path 'bg.jpg' %>);
	padding-top: 15px;
	margin-top: 15px !important;
}

.perfilcol1 {
	width: 48%;
	margin: 0 1%;
	float: left;
}

.perfilcol2 {
	width: 47%;
	margin: 0 2% 0 1%;
	float: left;
}

.perfildata {
	float:left;
	padding: 5px;
	clear: both;
}

.perfilimg {
	width: 150px;
	height: 150px;
	float: left;
	margin-bottom: 25px;
}


.perfilimg img {
	width: 150px;
	height: 150px;
	border: 1px solid #adc010;
	float: left;
}

.perfilexp {
	float: left;
	width: -webkit-calc(100% - 160px);
	width: -moz-calc(100% - 160px);
	width: calc(100% - 160px);
	margin-left: 10px;
}

.perfilexp h1 {
	font-family: 'bebas_neueregular', sans-serif;
	letter-spacing: 1px;
	font-size: 15px;
	color:#ff7742;
	line-height: 1;
}

.perfilexp h2 {
	font-size: 13px;
	color:#F0F5D0;
	line-height: 1;
	margin-bottom: 10px;
}

.perfilexp h3 {
	font-size: 12px;
	color:#008D96;
	line-height: 1;
}

.perfilexp h4 {
	font-size: 12px;
	color:#fff;
	line-height: 1.2;
}

.perfilexp p {
	font-size: 11px;
	color:#A6A6A6;
	line-height: 1.2;
	margin-bottom: 15px;
}

.cajas {
    width: 100%;
    border: 1px solid #3d3d3d;
    border-bottom: 5px solid #6d6d6d;
    padding: 0 10px;
    margin: 10px 0 5px;
    background-color: #fff;
}

.cajas h5 {
    display: inline-block;
	font-family: 'bebas_neueregular', sans-serif;
    font-size: 1em;
    font-weight: 400;
    color: #fff;
    letter-spacing: 1px;
    padding: 5px 10px;
    background-color: #008D96;
    margin-bottom: 10px;
}

.ranking {
	border-bottom: 1px solid #F9A848;
}

.ranking:last-child {
	border:none;
}

.ranking h1 {
	font-family: 'bebas_neueregular', sans-serif;
    font-size: 1.1em;
	padding: 0 2px;
	width: 20px;
	background-color: #e48413;
	border-left: 3px solid #F9A848;
	text-align: center;
	color:#fff;
	margin-right: 3px;
	display: inline-block;
}

.ranking a {
	font-size: 12px;
	color:#008D96;
}

.ranking p {
	font-size: 11px;
	display: inline-block;
}

.medalla1 {
	width: 24px;
	height: 24px;
	background-image: url(<%= asset_path 'medalla1.png' %>);
	display: inline-block;
	float:right;
	vertical-align: middle;
}

.aportes h1 {
	font-family: 'bebas_neueregular', sans-serif;
	font-size: 20px;
	letter-spacing: 1.3px;
	color:#008D96;
	line-height: 1;
}

.aportes a {
	font-family: 'bebas_neueregular', sans-serif;
	font-size: 14px;
	letter-spacing: 1.1px;
	color:#ff7742;
	margin-bottom: 5px;
}

.checkrev {
	border-bottom: 1px dashed #1ab0b5;
}

.aportes {
	display: block;
}

.comperfil {
	margin-top: 10px;
	overflow: auto;
	max-height: 250px;
}

.datosaportes {
	margin-top: -10px;
	display: block;
}

.feeduserp {
	font: 1em/1.4 'Lato',sans-serif !important;
	font-size: 11px !important;
	line-height: 3 !important;
	color: #e48413 !important;

}

.feedtimep {
	font-size: 10px;
	color: #878787;
	line-height: 0;
	margin-bottom: 5px;
}

.feedcontentp {
	font-size: 11px;
	color: #3D3D3D;
	line-height: 1;
	padding-left: 35px;
	border-bottom: 1px solid #C9C9C9;
	padding-bottom: 5px;
	margin-bottom: 5px;
}

.minavatar2 {
	float: left;
	margin-right: 5px;
	vertical-align: middle;
}

.vermodelo {
	float:right;
	margin-left: 5px;
}

.mright {
	margin-right: 20px;
}

