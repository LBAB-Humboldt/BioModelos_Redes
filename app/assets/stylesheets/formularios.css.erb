.formalogin {
  background-color: #FAC205;
  font-size: 12px;
  width: 350px;
  height: auto;
  padding: 30px 18px 10px 15px;
  border-bottom: 5px solid #FAE49B;
  background-image: url(<%= asset_path 'arroww.png' %>);
  background-repeat: no-repeat;
  background-position: 327px top;
  float: right;
  margin-right: 130px;
}

.formasignin {
	background-color: #FAC205;
	font-size: 12px;
	width: 420px;
	height: auto;
	padding: 15px 18px 10px 15px;
	border-bottom: 5px solid #FAE49B;
  background-image: url(<%= asset_path 'arroww.png' %>);
  background-repeat: no-repeat;
  background-position: 387px top;
  float: right;
  margin-right: 30px;
}

.bbton {
	padding: 1px 10px;
	background-color: #1ab0b5;
	color:#fff;
	font-family: 'bebas_neueregular', sans-serif;
  text-transform: uppercase;
  font-size: 1.2em;
  letter-spacing: 2px;
  border:none;
  float: right;
  cursor:pointer;
}

.inputforma {
	width: 340px;
	margin-bottom: 10px;
	border:none;
	padding: 3px;
	border-right: 5px solid #FAE49B;
  font-family: 'Lato', sans-serif !important;
  color: #414243;
  font-style: italic;
  font-size: 12px;
  letter-spacing: 1px;
}

.inp2 {
  width: 410px;
}

.w227 {
  width: 227px;
}

.wauto {
  width: auto;
}

.formasignin p {
  line-height: 1;
  font-size: 12px;
  color:#fff;
  list-style: 1;
  margin-bottom: 15px;
}

.checkbox {
  float: left;
  vertical-align: middle;
  font-size: 11px;
  color:#fff;
  letter-spacing: .8px;
  margin-top: 3px;
  margin-right: 5px;
}

.avatarlink {
  float: left;
  font-size: 11px;
  text-transform: uppercase;
  text-decoration: none;
  font-weight: 700;
  color:#008D96;
  letter-spacing: .3px;
  margin-top: 3px;
  margin-right: 5px;
}

.linkss {
  text-decoration: none;
  color:#008D96;
  font-size: 11px;
  display: inline-block;
  margin: 5px 10px 0;
  text-align: center;
}

.inglink {
  margin-left: 0;
}

.nobgimg {
  padding-top: 5px;
  background-image: none;
  margin-right: 0;
}

.fondo1 {
  width: 100%;
  height: -moz-calc(100% - 131px);
  height: -webkit-calc(100% - 131px);
  height: calc(100% - 131px);
  background-image: url(<%= asset_path 'ranaf1.jpg' %>);
  background-repeat: no-repeat;
  background-size: cover;
}

.fondo2 {
  width: 100%;
  height: -moz-calc(100% - 131px);
  height: -webkit-calc(100% - 131px);
  height: calc(100% - 131px);
  background-image: url(<%= asset_path 'ranaf2.jpg' %>);
  background-repeat: no-repeat;
  background-size: cover;
}

.fondo3, .bg4 {
  width: 100%;
  height: -moz-calc(100% - 131px);
  height: -webkit-calc(100% - 131px);
  height: calc(100% - 131px);
  background-image: url(<%= asset_path 'plumas.jpg' %>);
  background-repeat: no-repeat;
  background-size: cover;
}

.fondo4 {
  width: 100%;
  height: -moz-calc(100% - 131px);
  height: -webkit-calc(100% - 131px);
  height: calc(100% - 131px);
  background-image: url(<%= asset_path 'ranaf3.jpg' %>);
  background-repeat: no-repeat;
  background-size: cover;
  position: absolute;
}

.fondo5 {
  background-image: url(<%= asset_path 'grillo.jpg' %>);
}

.faqshow {
  width: 415px;
  height: -moz-calc(100% - 30px);
  height: -webkit-calc(100% - 30px);
  height: calc(100% - 30px);
  float: right;
  margin-right: 50px;
  background-image: url(<%= asset_path 'fondobla.jpg' %>);
  padding: 15px;
  overflow: scroll;
}

.w600 {
  width: 615px;
}

.ilb {
  display: inline-block;
  margin-right: 100px;
  vertical-align: top;
  margin-bottom: 20px;
}

.faqtitle h1 {
  background-color: #f3523d;
  padding: 0 5px;
  display: inline-block;
  font-family: 'bebas_neueregular', sans-serif;
  font-size: 40px;
  letter-spacing: 3px;
  line-height: 1;
  color: #fff;
  margin-bottom: 10px;
}

.faqtitle h2 {
  display: inline-block;
  font-size: 18px;
  color:#f3523d;
  font-family: 'bebas_neueregular', sans-serif;
  letter-spacing: 1px;
  line-height: 1;
}

.faqtitle {
  border-bottom: 1px dashed #f3523d;
}

.qya {
  margin:10px 0;
  font-size: 12px;
  color:#5d5d5d;
}

.qya h3, .tituloformas {
  color:#1ab0b5;
  font-weight: 700;
}

.contacto {
  width: 700px;
  height: -webkit-calc(100% - 171px);
  height: -moz-calc(100% - 171px);
  height: calc(100% - 171px);
  position: absolute;
  top: 86px;
  right:50px;
  z-index: 250;
  background-image: url(<%= asset_path 'fondobla.jpg' %>);
  padding: 20px;
  overflow: auto;
}

.claverec {
  width: 600px;
  height: auto;
  position: relative;
  margin: 10% auto;
  z-index: 250;
  background-image: url(<%= asset_path 'fondobla.jpg' %>);
  padding: 50px;
  overflow: auto;
}

.mailimg {
  position: absolute;
  top:25px;
  right:25px;
  width: 191px;
  height: 196px;
  background: url(<%= asset_path 'mail.png' %>);
  z-index:20;
}

.contacto h2, .fondo4 h2 {
  width: 80%;
  font-weight: 300;
  font-size: 30px;
  line-height: 1em;
  color: #333;
  text-align: left;
  margin-bottom: 15px;
  text-transform: uppercase;
}

.forma {
  margin-bottom: 25px;
  border-bottom: 2px dotted #e73a00;
}

.forma input[type="text"],
.forma input[type="email"],
.forma input[type="tel"] ,
.forma input[type="password"]  {
  border:none;
  border-bottom: 1px solid #e73a00;
  padding: 3px;
  width: 400px;
  margin-bottom: 10px;
  color: #fff;
  font-weight: 300;
  letter-spacing: 1px;
  display: block;
  background: url(<%= asset_path 'bg.jpg' %>);
}

.forma textarea {
  border: 1px solid #e73a00;
  padding: 3px;
  width: 600px;
  margin-bottom:10px;
  color: #fff;
  font-weight: 300;
  letter-spacing: 1px;
  background: url(<%= asset_path 'bg.jpg' %>);
}

.boton1 {
  font-family: 'bebas_neueregular', sans-serif;
  font-size: 16px;
  width: 68px;
  height: 30px;
    cursor: pointer;
    float: right;
    margin-right: 93px;
    border:none;
    background-color: #e73a00;
    letter-spacing: 2px;
    color: #fff;
    border-bottom: 3px solid #e75e31;
}

.qya a {
  color: #f3523d;
}

.pointBox {
  background-color: #f29e0d;
  position: absolute;
  top: 200px;
  left: -moz-calc(50% - 125px);
  left: -webkit-calc(50% - 125px);
  left: calc(50% - 125px);
  width: 250px;
  z-index: 200;
}

.pointcomm {
    font-family: 'Lato',sans-serif !important;
    color: #424242;
    font-size: 12px;
    padding: 10px;
}

.pointcomm input {
  float: none;
  margin: 5px;
  }

.commeinput2 {
    border: 1px dashed #81E0E3;
    color: #414243;
    font-family: 'Lato',sans-serif !important;
    font-size: 12px;
    font-style: italic;
    letter-spacing: 1px;
    margin: 10px 0;
    padding: 3px;
    width: 224px;
}

.wrongbtn {
  font-size: 1.1em;
  letter-spacing: 2px;
  padding: 2px 3px 2px 0;
  text-align: center;
  background-color: #e75523;
  color: #fff !important;
  font-family: 'bebas_neueregular', sans-serif;
  cursor: pointer;
  z-index:220;
  border:none;
  -webkit-transition: all 0.3s;
  -moz-transition: all 0.3s;
  transition: all 0.3s;
}

.wrongbtn:before {
  padding: 2px 10px;
    background: url(<%= asset_path 'wrong.png' %>) no-repeat center rgba(0, 0, 0, 0.1);
    margin-right: 4px;
    height: 100%;
    content: '';
}

.wrongbtn:hover {
  background: #e73a00;
}

#point_lat, #point_lon {
  display: inline-block;
}
